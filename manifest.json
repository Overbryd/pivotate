{
  "name": "Pivotate",
  "version": "0.6",
  "manifest_version": 2,
  "description": "Submit to Pivotal Tracker with screenshot.",
  "icons":{
    "128":"icon-128.png",
    "48" :"icon-48.png",
    "16" :"icon-16.png"
  },
  "browser_action":{
    "default_icon":"icon-19.png",
    "default_title":"Pivotal Submit",
    "default_popup":"popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "permissions": [ "tabs", "http://*/", "https://*/", "https://www.pivotaltracker.com/services/*" ]
}
